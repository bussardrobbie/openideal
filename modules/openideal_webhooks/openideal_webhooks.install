<?php

/**
 * @file
 * Contain main update/install functionality.
 */

use Drupal\openideal_webhooks\Plugin\RulesAction\WebhookAction as PluginType;

/**
 * Implements hook_install().
 */
function openideal_webhooks_install() {
  // Adds to default plugins for webhooks.
  \Drupal::configFactory()->getEditable('webhooks.settings')
    ->set('plugins', [
      PluginType::OTHER => 'Other',
      PluginType::SLACK => 'Slack',
    ])
    ->save();
}
